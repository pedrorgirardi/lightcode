{
    "name": "lightcode",
    "displayName": "LightCode",
    "description": "Clojure & ClojureScript IDE",
    "version": "0.0.1",
    "publisher": "pedrorgirardi",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:lightcode.welcome"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "title": "Welcome",
                "category": "Light Code",
                "command": "lightcode.welcome"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "shadow-cljs": "^2.3.16",
        "vscode": "^1.1.16"
    }
}
